<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.service.taskreport.mapper.persistence.TaskExecutionReportPersistenceMapper">
    <resultMap id="TaskExecutionReportResultMap" type="TaskExecutionReport">
        <id property="id" column="task_execution_id"/>
        <id property="taskId", column = "task_id"/>
        <id property="startDateTime", column = "start_date_time"/>
        <id property="endDateTime", column = "end_date_time"/>
        <id property="executionTimeSeconds", column = "execution_time_seconds"/>
        <id property="errorMessage", column = "error_message"/>
        <id property="status", column = "status"/>
        <id property="insertDate", column = "insert_date"/>
        <id property="updateDate", column = "update_date"/>
    </resultMap>
    <select id="findById" resultType="TaskExecutionReport" resultMap="TaskExecutionReportResultMap">
        SELECT * FROM task_execution_report WHERE task_execution_id = #{id}
    </select>
</mapper>